{
	"name": "site",
	"version": "1.2.43",
	"private": true,
	"license": "MIT",
	"scripts": {
		"start": "gatsby serve",
		"develop": "npm run clean && gatsby develop -o",
		"clean": "rimraf public .cache",
		"build": "npm run clean && run-p build:*",
		"build:site": "gatsby build",
		"build:resume": "mkdirp public && node scripts/export-resume.js src/resume.json public/resume.pdf",
		"test:unit": "jest --runInBand",
		"test:e2e": "CYPRESS_SUPPORT=y npm run build && start-server-and-test :9000 cy:run",
		"spellcheck": "mdspell --en-us -anxr posts/*/*.md",
		"test": "run-p test:*",
		"lint": "eslint generators scripts src test",
		"cy:run": "(is-ci && cypress run --browser chrome --record) || cypress run --browser chrome --record false",
		"new:post": "yo ./generators/post"
	},
	"dependencies": {
		"gatsby": "^2.22.11",
		"gatsby-image": "^2.4.5",
		"gatsby-plugin-feed": "^2.5.3",
		"gatsby-plugin-less": "^3.2.2",
		"gatsby-plugin-manifest": "^2.4.9",
		"gatsby-plugin-offline": "^3.2.7",
		"gatsby-plugin-react-helmet": "^3.3.2",
		"gatsby-plugin-robots-txt": "^1.5.0",
		"gatsby-plugin-sharp": "^2.6.6",
		"gatsby-plugin-sitemap": "^2.4.2",
		"gatsby-remark-copy-linked-files": "^2.3.3",
		"gatsby-remark-draw": "^1.0.16",
		"gatsby-remark-embed-snippet": "git+https://github.com/kvadevack/gatsby-remark-embed-snippet.git",
		"gatsby-remark-embed-video": "^3.0.10",
		"gatsby-remark-external-links": "0.0.4",
		"gatsby-remark-images": "^3.3.8",
		"gatsby-remark-prismjs": "^3.5.2",
		"gatsby-remark-responsive-iframe": "^2.4.3",
		"gatsby-source-filesystem": "^2.3.8",
		"gatsby-transformer-remark": "^2.8.13",
		"gatsby-transformer-sharp": "^2.5.2",
		"less": "^3.11.1",
		"mermaid": "^8.5.1",
		"mermaid.cli": "^0.5.1",
		"moment": "^2.26.0",
		"prismjs": "^1.17.1",
		"prop-types": "^15.6.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.0.0",
		"semantic-ui-less": "^2.4.1",
		"semantic-ui-react": "^0.88.0"
	},
	"devDependencies": {
		"@babel/core": "^7.9.6",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@testing-library/cypress": "^6.0.0",
		"@testing-library/jest-dom": "^5.8.0",
		"@testing-library/react": "^10.0.4",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.0.1",
		"babel-plugin-remove-test-ids": "^1.1.0",
		"babel-preset-gatsby": "^0.4.7",
		"codecov": "^3.7.0",
		"cypress": "^4.6.0",
		"cypress-image-snapshot": "^3.1.1",
		"eslint": "^6.5.1",
		"eslint-config-standard": "^14.1.1",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-standard": "^4.0.1",
		"execa": "^4.0.2",
		"gatsby-cypress": "^0.4.2",
		"generator-license": "^5.4.0",
		"gh-pages": "^2.1.1",
		"identity-obj-proxy": "^3.0.0",
		"is-ci": "^2.0.0",
		"jest": "^26.0.1",
		"jsonresume-theme-rocketspacer": "git+https://github.com/kvadevack/jsonresume-theme-rocketspacer.git",
		"markdown-spellcheck": "^1.3.1",
		"mime": "^2.4.5",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^3.1.0",
		"regenerator-runtime": "^0.13.3",
		"rimraf": "^3.0.0",
		"semantic-release": "^17.0.8",
		"slugify": "^1.3.4",
		"start-server-and-test": "^1.11.0",
		"yeoman-generator": "^4.10.1",
		"yo": "^3.1.0"
	}
}
