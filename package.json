{
	"name": "site",
	"version": "1.0.1",
	"private": true,
	"license": "MIT",
	"scripts": {
		"start": "gatsby serve",
		"develop": "npm run clean && gatsby develop -o",
		"clean": "rimraf public .cache",
		"build": "npm run clean && run-p build:*",
		"build:site": "gatsby build",
		"build:resume": "mkdirp public && node scripts/export-resume.js src/resume.json public/resume.pdf",
		"test:unit": "jest --runInBand",
		"test:e2e": "CYPRESS_SUPPORT=y npm run build && start-server-and-test :9000 cy:run",
		"spellcheck": "mdspell --en-us -anxr posts/*/*.md",
		"test": "run-p test:*",
		"lint": "eslint generators scripts src test",
		"cy:run": "cypress run",
		"new:post": "yo ./generators/post"
	},
	"dependencies": {
		"gatsby": "^2.10.0",
		"gatsby-image": "^2.2.1",
		"gatsby-plugin-feed": "^2.3.0",
		"gatsby-plugin-less": "^2.1.0",
		"gatsby-plugin-manifest": "^2.2.0",
		"gatsby-plugin-offline": "^2.2.0",
		"gatsby-plugin-react-helmet": "^3.1.0",
		"gatsby-plugin-robots-txt": "^1.3.0",
		"gatsby-plugin-sharp": "^2.2.1",
		"gatsby-plugin-sitemap": "^2.2.0",
		"gatsby-remark-copy-linked-files": "^2.1.0",
		"gatsby-remark-draw": "^1.0.16",
		"gatsby-remark-embed-snippet": "git+https://github.com/kvadevack/gatsby-remark-embed-snippet.git",
		"gatsby-remark-embed-video": "^1.7.1",
		"gatsby-remark-external-links": "0.0.4",
		"gatsby-remark-images": "^3.1.0",
		"gatsby-remark-prismjs": "^3.2.0",
		"gatsby-remark-responsive-iframe": "^2.0.10",
		"gatsby-source-filesystem": "^2.0.16",
		"gatsby-transformer-remark": "^2.2.0",
		"gatsby-transformer-sharp": "^2.1.10",
		"less": "^3.9.0",
		"mermaid": "^8.0.0",
		"mermaid.cli": "^0.5.1",
		"moment": "^2.23.0",
		"prismjs": "^1.15.0",
		"prop-types": "^15.6.2",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-helmet": "^5.2.0",
		"semantic-ui-less": "^2.4.1",
		"semantic-ui-react": "^0.87.2"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@semantic-release/changelog": "^3.0.2",
		"@semantic-release/git": "^7.0.8",
		"@testing-library/cypress": "^4.0.0",
		"@testing-library/react": "^8.0.1",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.3.1",
		"babel-plugin-remove-test-ids": "^1.1.0",
		"babel-preset-gatsby": "^0.1.6",
		"codecov": "^3.2.0",
		"cypress": "^3.1.4",
		"eslint": "^5.15.0",
		"eslint-config-standard": "^12.0.0",
		"eslint-plugin-cypress": "^2.2.1",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-node": "^9.0.0",
		"eslint-plugin-promise": "^4.0.1",
		"eslint-plugin-react": "^7.12.4",
		"eslint-plugin-standard": "^4.0.0",
		"execa": "^1.0.0",
		"gatsby-cypress": "^0.1.5",
		"generator-license": "^5.4.0",
		"gh-pages": "^2.0.1",
		"greenkeeper-lockfile": "^1.15.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.3.1",
		"jest-dom": "^3.0.0",
		"jsonresume-theme-rocketspacer": "git+https://github.com/kvadevack/jsonresume-theme-rocketspacer.git",
		"markdown-spellcheck": "^1.3.1",
		"mime": "^2.4.0",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^1.11.0",
		"regenerator-runtime": "^0.13.1",
		"rimraf": "^2.6.3",
		"semantic-release": "^15.13.3",
		"slugify": "^1.3.4",
		"start-server-and-test": "^1.9.1",
		"yeoman-generator": "^4.0.1",
		"yo": "^2.0.6"
	}
