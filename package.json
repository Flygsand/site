{
	"name": "site",
	"version": "1.5.14",
	"private": true,
	"license": "MIT",
	"scripts": {
		"start": "gatsby serve",
		"develop": "npm run clean && gatsby develop -o",
		"clean": "rimraf public .cache",
		"build": "npm run clean && run-p build:*",
		"build:site": "gatsby build",
		"build:resume": "mkdirp public && node scripts/export-resume.js src/resume.json public/resume.pdf",
		"test:unit": "jest --runInBand",
		"test:e2e": "CYPRESS_SUPPORT=y npm run build && start-server-and-test :9000 cy:run",
		"spellcheck": "mdspell --en-us -anxr posts/*/*.md",
		"test": "run-p test:*",
		"lint": "NODE_ENV=test eslint --max-warnings=0 generators scripts src test",
		"cy:run": "(is-ci && cypress run --browser chrome --record) || cypress run --browser chrome --record false",
		"new:post": "yo ./generators/post"
	},
	"dependencies": {
		"gatsby": "^4.24.6",
		"gatsby-plugin-feed": "^4.17.0",
		"gatsby-plugin-image": "^2.17.0",
		"gatsby-plugin-less": "^6.17.0",
		"gatsby-plugin-manifest": "^4.17.0",
		"gatsby-plugin-offline": "^5.17.0",
		"gatsby-plugin-react-helmet": "^5.17.0",
		"gatsby-plugin-robots-txt": "^1.7.1",
		"gatsby-plugin-sharp": "^4.17.0",
		"gatsby-plugin-sitemap": "^5.17.0",
		"gatsby-remark-copy-linked-files": "^5.17.0",
		"gatsby-remark-draw": "^1.0.16",
		"gatsby-remark-embed-snippet": "^7.17.0",
		"gatsby-remark-embed-video": "^3.1.4",
		"gatsby-remark-external-links": "0.0.4",
		"gatsby-remark-images": "^6.17.0",
		"gatsby-remark-images-medium-zoom": "^1.7.0",
		"gatsby-remark-prismjs": "^6.17.0",
		"gatsby-remark-responsive-iframe": "^5.17.0",
		"gatsby-source-filesystem": "^4.17.0",
		"gatsby-transformer-remark": "^5.17.0",
		"gatsby-transformer-sharp": "^4.17.0",
		"less": "^4.1.3",
		"mermaid": "^9.1.2",
		"mermaid.cli": "^0.5.1",
		"moment": "^2.29.3",
		"prismjs": "^1.28.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet": "^6.1.0",
		"semantic-ui-less": "^2.4.1",
		"semantic-ui-react": "^2.1.3"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/cypress": "^8.0.3",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^28.1.1",
		"babel-preset-gatsby": "^2.16.0",
		"cypress": "^10.2.0",
		"cypress-image-snapshot": "^4.0.1",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-gatsby": "github:Flygsand/gatsby-plugin-eslint-config",
		"execa": "5.1.1",
		"gatsby-cypress": "^2.17.0",
		"generator-license": "^5.5.0",
		"gh-pages": "^4.0.0",
		"identity-obj-proxy": "^3.0.0",
		"is-ci": "^3.0.1",
		"jest": "^28.1.1",
		"jsonresume-theme-rocketspacer": "github:Flygsand/jsonresume-theme-rocketspacer",
		"markdown-spellcheck": "^1.3.1",
		"mime": "^3.0.0",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^15.1.1",
		"regenerator-runtime": "^0.13.9",
		"rimraf": "^3.0.0",
		"semantic-release": "^19.0.2",
		"slugify": "^1.6.4",
		"start-server-and-test": "^1.14.0",
		"yeoman-generator": "^5.4.2",
		"yo": "^4.3.0"
	}
}
